#: ../src/context_menu.c:39
msgid "Do not hide on join"
msgstr "Não ocultar ao entrar"

#: ../src/context_menu.c:41
msgid "Hide on join"
msgstr "Ocultar ao entrar"

#: ../src/conversation_handler.c:56
msgid "Activate tabs to avoid flickering!\n"
msgstr "Ative as abas para evitar a luz intermitente!\n"

#: ../src/hide_chat.c:43
